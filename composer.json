{
	"name"        : "medfreeman/usine.ch",
	"description" : "The 2016 version of usine.ch website",
	"authors"     : [
		{
			"name"    : "Mehdi Lahlou",
			"email"   : "mehdi.lahlou@free.fr",
			"homepage": "https://github.com/medfreeman/"
		}
	],
	"type"        : "project",
	"minimum-stability": "dev",
	"repositories": [
		{
			"type": "composer",
			"url" : "http://wpackagist.org"
		},
		{
			"type": "package",
			"package": {
				"name": "medfreeman/wp-usine",
				"type": "wordpress-theme",
				"version": "1.1.3",
				"dist": {
					"url": "https://github.com/medfreeman/wp-usine/releases/download/1.1.3/wp-usine-1.1.3.zip",
					"type": "zip"
				}
			}
		},
		{
		"type": "package",
			"package": {
				"name": "medfreeman/wp-bla",
				"type": "wordpress-plugin",
				"version": "*",
				"source": {
					"type": "git",
					"url": "git@github.com:medfreeman/wp-bla.git",
					"reference": "master"
				}
			}
		},
		{
		"type": "package",
			"package": {
				"name": "medfreeman/wp-voxusini",
				"type": "wordpress-plugin",
				"version": "*",
				"source": {
					"type": "git",
					"url": "git@github.com:medfreeman/wp-voxusini.git",
					"reference": "master"
				}
			}
		}
	],
	"config"      : {
		"vendor-dir":  "vendor"
	},
	"require"     : {
		"johnpbloch/wordpress"                        : "*",
		"medfreeman/wp-usine"                         : "*",
		"wpackagist-plugin/admin-menu-editor"         : "*",
		"wpackagist-plugin/adminimize"                : "*",
		"afragen/github-updater"                      : "*",
		"wpackagist-plugin/leaflet-maps-marker"       : "*",
		"wpackagist-plugin/members"                   : "*",
		"wpackagist-plugin/meta-box"                  : "*",
		"wpackagist-plugin/remove-image-links"        : "*",
		"wpackagist-plugin/simple-link-list-widget"   : "*",
		"wpackagist-plugin/taxonomy-images"           : "*",
		"wpackagist-plugin/wordpress-importer"        : "*",
		"wpackagist-plugin/wp-ajaxify-comments"       : "*",
		"medfreeman/wp-bla"                           : "*",
		"wpackagist-plugin/wp-less"                   : "*",
		"medfreeman/wp-voxusini"                      : "*"
	},
	"require-dev" : {
		"wpackagist-plugin/core-control"              : "*",
		"wpackagist-plugin/debug-bar"                 : "*",
		"wpackagist-plugin/log-deprecated-notices"    : "*",
		"wpackagist-plugin/regenerate-thumbnails"     : "*"
	},
	"extra"       : {
		"wordpress-install-dir": "htdocs/wordpress",
		"installer-paths": {
			"htdocs/wordpress/wp-content/plugins/{$name}/" : ["type:wordpress-plugin"],
			"htdocs/wordpress/wp-content/themes/{$name}/"  : ["type:wordpress-theme"]
		}
	}
}
